{
  "name": "eslint-plugin-node-roikoren",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/roikoren755/eslint-plugin-node.git"
  },
  "author": "Roi Koren",
  "eslintIgnore": [
    "dist",
    "node_modules"
  ],
  "license": "MIT",
  "main": "./index.js",
  "files": [
    "**/*.js"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.prod.json",
    "clean": "tsc --build --clean && rimraf .nyc_output coverage",
    "coverage": "nyc report -r lcov",
    "deduplicate": "yarn-deduplicate -s fewer",
    "lint": "eslint . --cache",
    "new": "ts-node scripts/new-rule",
    "postrelease": "yarn clean",
    "prepare": "husky install",
    "prerelease": "yarn build",
    "release": "changeset publish",
    "test": "nyc mocha tests/**/*.ts --reporter dot --timeout 4000",
    "type": "tsc",
    "update": "ts-node scripts/update"
  },
  "resolutions": {
    "prettier": "2.5.0"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "5.5.0",
    "eslint-plugin-es-roikoren": "0.0.7",
    "ignore": "5.1.9",
    "is-core-module": "2.8.0",
    "minimatch": "3.0.4",
    "resolve": "1.20.0",
    "semver": "7.3.5"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.2",
    "@changesets/cli": "2.18.1",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/is-core-module": "2.2.0",
    "@types/minimatch": "3.0.5",
    "@types/mocha": "9.0.0",
    "@types/resolve": "1.20.1",
    "@types/semver": "7.3.9",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "@typescript-eslint/types": "5.5.0",
    "@typescript-eslint/typescript-estree": "5.5.0",
    "camelcase": "6.2.1",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-eslint-plugin": "4.0.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unicorn": "39.0.0",
    "fast-glob": "3.2.7",
    "globals": "13.12.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "mocha": "9.1.3",
    "nyc": "15.1.0",
    "prettier": "2.5.0",
    "type-fest": "2.8.0",
    "typescript": "4.5.2",
    "yarn-deduplicate": "3.1.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --quiet --cache"
    ]
  }
}
